    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/multer": "^1.4.7",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "apidoc": "^0.53.1",
    "async-await-mutex-lock": "^1.0.7",
